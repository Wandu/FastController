{
    "name": "wandu/router",
    "description": "FastRoute with PSR-7 Wrapper Library.",
    "keywords": ["wandu", "router", "psr7", "middleware"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "psr/container": "^1",
        "psr/http-message": "^1",
